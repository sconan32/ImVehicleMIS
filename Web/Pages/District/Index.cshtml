@page
@model Web.Pages.District.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.DistrictItem[0].Address)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DistrictItem[0].DivisionType)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DistrictItem[0].VersionNumber)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DistrictItem[0].Metadata)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DistrictItem[0].Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DistrictItem[0].CreationDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DistrictItem[0].ModificationDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DistrictItem[0].CreateBy)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DistrictItem[0].ModifyBy)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DistrictItem[0].Status)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.DistrictItem) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DivisionType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VersionNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Metadata)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModificationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifyBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
